{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.1.0",
    "generators": {
      "v2.0": {
        "generatorName": "typescript-axios",
        "output": "./openapi",
        "inputSpec": "http://localhost:3000/api/docs-yaml",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
