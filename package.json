  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.2.1",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
